@using MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Startup
@{
    ViewBag.CurrentPageName = AppAreaNamePageNames.Common.DemoUiComponents;
}
@section Styles
{
    <environment names="Development">
        <link rel="stylesheet" href="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/DateAndTimePickers/DemoDateAndTimePickers.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/FileUpload/FileUpload.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/Selections/Selections.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/InputMask/InputMask.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/Editor/Editor.css" asp-append-version="true" />
        
        <link rel="stylesheet" href="~/lib/blueimp-file-upload/css/jquery.fileupload.css" asp-append-version="true" />

        <link rel="stylesheet" href="~/lib/summernote/dist/summernote.css" asp-append-version="true" />
    </environment>

    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/view-resources/Areas/AppAreaName/Views/_Bundles/demo-ui-components.min.css" asp-append-version="true" />
        
        
        <link rel="stylesheet" href="~/lib/blueimp-file-upload/css/jquery.fileupload.min.css" asp-append-version="true" />

        <link rel="stylesheet" href="~/lib/summernote/dist/summernote.min.css" asp-append-version="true" />
    </environment>
}

@section Scripts
{
    <environment names="Development">
        <script src="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/DateAndTimePickers/DemoDateAndTimePickers.js" asp-append-version="true"></script>
        <script src="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/FileUpload/FileUpload.js" asp-append-version="true"></script>
        <script src="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/Selections/Selections.js" asp-append-version="true"></script>
        <script src="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/InputMask/InputMask.js" asp-append-version="true"></script>
        <script src="~/view-resources/Areas/AppAreaName/Views/DemoUiComponents/Editor/Editor.js" asp-append-version="true"></script>
        
        <script src="~/lib/inputmask/dist/jquery.inputmask.bundle.js" asp-append-version="true"></script>
        
        <script src="~/lib/blueimp-file-upload/js/vendor/jquery.ui.widget.js" asp-append-version="true"></script>
        <script src="~/lib/blueimp-file-upload/js/jquery.iframe-transport.js" asp-append-version="true"></script>
        <script src="~/lib/blueimp-file-upload/js/jquery.fileupload.js" asp-append-version="true"></script>
        
        <script src="~/lib/summernote/dist/summernote.js" asp-append-version="true"></script>
    </environment>

    <environment names="Staging,Production">
        <script src="~/view-resources/Areas/AppAreaName/Views/_Bundles/demo-ui-components.min.js" asp-append-version="true"></script>
        
        <script src="~/lib/inputmask/dist/min/jquery.inputmask.bundle.min.js" asp-append-version="true"></script>
        
        <script src="~/lib/blueimp-file-upload/js/vendor/jquery.ui.widget.min.js" asp-append-version="true"></script>
        <script src="~/lib/blueimp-file-upload/js/jquery.iframe-transport.min.js" asp-append-version="true"></script>
        <script src="~/lib/blueimp-file-upload/js/jquery.fileupload.min.js" asp-append-version="true"></script>
        
        <script src="~/lib/summernote/dist/summernote.min.js" asp-append-version="true"></script>
    </environment>
}
<div class="row margin-bottom-5">
    <div class="col-xs-6">
        <div class="page-head">
            <div class="page-title">
                <h1>
                    <span>@L("DemoUiComponents")</span>
                </h1>
            </div>
        </div>
    </div>
</div>

<div class="alert alert-success alert-dismissable">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
    @L("DemoUiComponents_Info")<br />
    <a href="http://keenthemes.com/preview/metronic/theme/admin_4_material_design/index.html" class="alert-link" target="_blank">@L("DemoUiComponents_Info_Metronic_Link_Text")</a>
</div>

@Html.Partial("_DateAndTimePickers")

@Html.Partial("_FileUpload")

@Html.Partial("_Selections")

@Html.Partial("_InputMask")

@Html.Partial("_Editor")